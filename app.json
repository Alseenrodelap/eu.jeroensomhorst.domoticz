{
  "id": "eu.jeroensomhorst.domoticz",
  "version": "0.0.5",
  "compatibility": ">=1.5.0",
  "sdk": 2,
  "name": {
    "en": "Domoticz app "
  },
  "description": {
    "en": "Add support for Domoticz devices"
  },
  "category": [
    "energy","tools"
  ],
  "permissions": [],
  "images": {
    "large": "assets/domoticz_large.jpg",

    "small": "assets/domoticz_small.jpg"
  },
  "author": {
    "name": "Jeroen Somhorst",
    "email": "j.somhorst@gmail.com"
  },
  "capabilities":{
    "fan_speed": {
      "type" : "number",
      "title" : {
        "en": "Fanspeed",
        "nl": "Ventilator snelheid"
      },
      "getable": true,
      "setable": false,
      "units": {
        "en": "RPM",
        "nl": "RPM"
      }
    }
  },
  "drivers": [
    {
      "id": "domoticz_device",
      "name": "Domotic Device",
      "class": "other",
      "capabilities": [],
      "images": {
        "large": "assets/driver/domoticz_large.jpg",
        "small": "assets/driver/domoticz_small.jpg"
      },
      "pair": [{
          "id": "domoticzsettings",
          "navigation": {
            "next": "list_devices"
          }
        },{
        "id": "list_devices",
        "template": "list_devices",
        "navigation": {
          "next": "add_my_devices"
        }
      },
        {
          "id": "add_my_devices",
          "template": "add_devices"
        }

      ]
    }
]
}